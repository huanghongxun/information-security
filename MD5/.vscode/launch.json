    "version": "0.2.0",
    "configurations": [
        {
            "name": "md5",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/md5",
            "args": ["a"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "set output-radix 16",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable printing long string",
                    "text": "set print elements 0",
                    "ignoreFailures": true
                }
            ]
        }
    ]
